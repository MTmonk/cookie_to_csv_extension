{
  "manifest_version": 3,
  "name": "Cookie Exporter",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "downloads"
  ],
  "host_permissions": [
    "https://*.louisvuitton.com/*", // Domains must change for other websites
    "http://*.louisvuitton.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
