{
  "manifest_version": 3,
  "name": "Cookie Exporter",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "downloads",
    "http://*/*",
    "https://*/*"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
